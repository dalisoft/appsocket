{
  "name": "appsocket",
  "version": "0.0.1",
  "description": "Lightweight, fast & simple WebSocket client",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "rm -rf build && preact build --template src/template.html",
    "serve": "npm run build && preact serve",
    "dev": "preact watch --template src/template.html",
    "lint": "eslint src",
    "app": "npm run build && deskgap ."
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-synacor": "^3.0.4",
    "if-env": "^1.0.4",
    "preact-cli": "^2.2.1",
    "@namics/stylelint-bem": "^6.0.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "stylelint": "^10.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "deskgap": "^0.1.0",
    "file-saver": "^2.0.2",
    "preact": "^8.4.2",
    "preact-virtual-list": "^0.3.1",
    "unistore": "^3.4.1"
  }
}
